                <add>
                    <div>
                        <sub>
                            <var>localtemperature</var>
                            <num>32</num>
                        </sub>
                        <num>1.8</num>
                    </div>
                    <num>273.15</num>
                </add>
                <num>0</num>
            </round>
            <str>{white}K</str>
            <str> {green}{humidity}{white}% Humidity</str>
        </line>
        <line>
            <icon>
                <str>minecraft:skull</str>

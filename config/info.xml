    </lines>
    <lines at = "middleleft">
        <line>
            <if>
                <equal>
                    <var>slimechunk</var>
                    <str>true</str>
                </equal>
                <concat>
                    <icon>
                            <str>minecraft:slime_ball</str>
                    </icon>
                    <str> This is a {aqua}Slime {white}chunk.</str>
                </concat>
            </if>
        </line>
        <line>
            <if>
                <and>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>-2</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkx</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                    <or>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>-2</num>
                        </equal>
                        <equal>
                            <modi>
                                <var>chunkz</var>
                                <num>3</num>
                            </modi>
                            <num>1</num>
                        </equal>
                    </or>
                </and>
                <concat>
                    <icon>
                        <str>minecraft:diamond_pickaxe</str>
                    </icon>
                    <str>  This is an {aqua}Ore{white} chunk.</str>
                </concat>
            </if>
        </line>
        <line>
            <if>
                <and>
                    <equal>
                        <var>chunkoffsetx</var>
                        <num>7</num>
                    </equal>
                    <equal>
                        <var>chunkoffsetz</var>
                        <num>7</num>
                    </equal>
                </and>
                <concat>
                        <icon>
                                <str>minecraft:redstone</str>
                        </icon>
                <str> You are at the {aqua}center {white}of the chunk.</str>
                </concat>
            </if>
        </line>
    </lines>
<lines at="topcenter">
        <line>
            <icon>
                <str>sereneseasons:ss_icon</str>
            </icon>
            <op>
                <str>eq</str>
                <var>sereneseasonscurrentseasonord</var>
                <num>0</num>
                <num>1</num>
                <num>2</num>
                <num>3</num>
                <str> {aqua}Spring{white}</str>
                <str> {aqua}Summer{white}</str>
                <str> {aqua}Autumn{white}</str>
                <str> {aqua}Winter{white}</str>
                <str> {aqua}Error{white}</str>
            </op>

            <str>, Day {aqua}</str>
            <add>
                <var>sereneseasonsdayofseason</var>
                <num>1</num>
            </add>
        </line>

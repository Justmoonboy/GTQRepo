<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="topleft">
        <line>
            <str>{pingicon} Player Info: {green}{username} {white}FPS: {aqua}{FPS}</str>
        </line>
            <line>
        <str>{green}GT Quantum v1.0.8</str>
    </line>
        <line>
            <icon>
                <str>minecraft:clock</str>
            </icon>
            <str> Minecraft Year {green}</str>
            <add>
                <round>
                    <div>
                        <sub>
                            <var>day</var>
                            <modi>
                                <var>day</var>
                                <num>365</num>
                            </modi>
                        </sub>
                        <num>365</num>
                    </div>
                    <num>0</num>
                </round>
                <num>1</num>
            </add>
            <str> {white}Day {green}</str>
            <modi>
                <var>day</var>
                <num>365</num>
            </modi>
            <str>{white} Time: {green}{mctime12} </str>
